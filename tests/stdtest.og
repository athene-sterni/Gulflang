\=
 If you run this program you should see the
 value dump of an integer 42. If the program
 aborts before doing that a test-case has
 failed. 

 Please report failing test-cases to the bug-tracker.

 (c) the Octogulf people
\

Main {
  AEQ(1 1)

  \= ASSIGNMENT \
  :a 5
  AEQ(a 5)
  :$bc 3
  AEQ(3 $bc)
  :Ulse 9
  AEQ(Ulse 9)
  :b 5
  AEQ(ab)
  AEQ(+a+ba ++aaa)

  \= BASIC ARITHMETIC \
   \= INTEGER \
   AEQ(+1 9 10)
   AEQ(+_1 9 8)
   AEQ(-5 3 2)
   AEQ(-5 _3 8)
   AEQ(*5 3 15)
   AEQ(*5 _3 _15)
   AEQ(/15 3 5)
   AEQ(/15 _3 _5)

  \= STRING OPERATORS \
   AEQ(+"Hi " "there" "Hi there")

  \= COMPARISONS \
   \= INTEGER \
   if >1 0 { } else { AEQ(1 0) }
   if <1 0 { AEQ(1 0) } else { }
   if =9 9 { } else { AEQ(1 0) }
   if =8 9 { AEQ(1 0) } else { }

   \= STRINGS \
   if >"cc" "ca" { } else { AEQ(1 0) }
   if <"cc" "ca" { AEQ(1 0) } else { }
   if ="cc" "cc" { } else { AEQ(1 0) }
   if ="cc" "ca" { AEQ(1 0) } else { }


  \= BASIC OOP STUFF \
  AEQ(.New()~>a 5)

  \= 42 means all tests passed \
  ValDump(42)
}

New {
  obj { a 5 }
}
